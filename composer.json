{
    "name": "boyo/wp-make",
    "description": "A WordPress 5 blank development environment + theme boilerplate",
    "type": "project",
    "homepage": "https://github.com/boyanraichev/wp-make",
    "license": "MIT",
    "authors": [
        {
            "name": "Boyan Raichev",
            "email": "myself@boyanr.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "boyo/wp5-bang": ">=1.1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/boyanraichev/wp5-bang.git"
        }
    ],
    "autoload": {
		"psr-4": {
			"Theme\\": "web/themes/wp5-blank/src/",
			"Plugin\\": "web/plugins/wp5-bang/src/"
    		}
	}
}
